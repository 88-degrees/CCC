    android:id="@+id/nav_graph"
    app:startDestination="@id/calculatorFragment">

    <fragment
        android:id="@+id/calculatorFragment"
        android:name="mustafaozhan.github.com.mycurrencies.ui.main.calculator.CalculatorFragment"
